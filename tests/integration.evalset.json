{
  "eval_set_id": "places_search_integration_suite",
  "description": "Integration tests for AI-Powered Nearby Places Search (Day 4b)",
  "eval_cases": [
    {
      "eval_id": "basic_coffee_search_tokyo",
      "description": "Basic search for coffee shops in Tokyo",
      "conversation": [
        {
          "user_content": {
            "parts": [
              {
                "text": "Find nearby places in Tokyo for someone who likes coffee. Provide specific recommendations with names, brief descriptions, and why they would enjoy them."
              }
            ]
          },
          "final_response": {
            "parts": [
              {
                "text": "Based on your interest in coffee, here are the top recommendations in Tokyo"
              }
            ]
          },
          "intermediate_data": {
            "tool_uses": [
              {
                "name": "google_search",
                "args": {
                  "query": "coffee shops Tokyo"
                }
              },
              {
                "name": "get_place_category_boost",
                "args": {
                  "category": "coffee",
                  "preferences": "coffee"
                }
              }
            ]
          }
        }
      ]
    },
